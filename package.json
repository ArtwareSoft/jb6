{
  "name": "jb6-monorepo",
  "private": true,
  "version": "1.251226.0",
  "description": "JB6 core + plugins (pure-JS workspaces mono-repo)",
  "type": "module",
  "scripts": {
    "test": "node packages/testing/run-tests-cli.js all",
    "test-via-web": "start http://localhost:8083/packages/testing/tests.html",
    "local": "node --experimental-vm-modules --expose-gc packages/testing/jb-web-server.js",
    "jb6:bumpVer": "node scripts/bump-calver.js",
    "jb6:publish": "bash scripts/publish.sh",
    "bumpPub": "npm run jb6:bumpVer && npm run jb6:publish",
    "update-vscode-ext": "npm run jb6:bumpVer && bash scripts/update-vscode-ext.sh",
    "publish-vscode-ext": "start https://marketplace.visualstudio.com/manage/publishers/",
    "update-test-project": "bash scripts/update-test-proj.sh"
  },
  "workspaces": [
    "packages/*"
  ],
  "imports": {
    "#jb6/*": "./packages/*"
  },
  "devDependencies": {
    "autoprefixer": "^10.4.22",
    "esbuild": "^0.25.11",
    "express": "^5.1.0",
    "globby": "^14.1.0",
    "lucide": "^0.546.0",
    "puppeteer-core": "^24.30.0",
    "@tailwindcss/cli": "^4.1.17",
    "@tailwindcss/postcss": "^4.1.17",
    "postcss": "^8.5.6",
    "tailwindcss": "^4.1.17"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "array-flatten": "^3.0.0"
  }
}
